{
  "manifest_version": 3,
  "name": "Keyword Based Navigator",
  "version": "1.1",
  "description": "Navigate to URLs based on shortcuts and keywords",
  "permissions": ["storage", "downloads", "notifications", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "p"
  },
  "options_page": "options.html",
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "48": "icon.png"
    }
  }
}